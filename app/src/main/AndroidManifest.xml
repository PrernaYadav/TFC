<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.infosolution.dev.tfc">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginMailActivity" />
        <activity android:name=".activities.SignupMailActivity" />
        <activity android:name=".activities.LoginBusinessActivity" />
        <activity android:name=".activities.SignupBusiActivity" />
        <activity android:name=".activities.UserRegisterActivity" />
        <activity android:name=".activities.UserRegNext" />
        <activity android:name=".activities.ProductPageActivity"></activity>
    </application>

</manifest>